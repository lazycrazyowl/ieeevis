<http://ieeevis.tw.rpi.edu/source/ieeevis-tw-rpi-edu/dataset/movie-characters/version/2013-Apr-18/conversion/enhancement/1>
   a conversion:LayerDataset, void:Dataset;

   conversion:base_uri           "http://ieeevis.tw.rpi.edu"^^xsd:anyURI;
   conversion:source_identifier  "ieeevis-tw-rpi-edu";
   conversion:dataset_identifier "movie-characters";
   conversion:version_identifier "2013-Apr-18";
   conversion:enhancement_identifier "1";

   conversion:conversion_process [
      a conversion:EnhancementConversionProcess;
      conversion:enhancement_identifier "1";

      dcterms:creator <http://purl.org/twc/id/machine/lebot/MacBookPro6_2#lebot>;
      dcterms:created "2013-04-18T01:02:02-04:00"^^xsd:dateTime;


      #conversion:enhance [
      #   ov:csvRow 2;
      #   a conversion:DataStartRow;
      #];

      conversion:delimits_cell ",";
      #conversion:delimits_cell "	"; # tab
      #conversion:delimits_cell "|";   # pipe
      #conversion:delimits_cell ",";   # comma

      conversion:interpret [
         conversion:symbol        "","-";
         conversion:interpretation conversion:null; 
      ];
      conversion:enhance [
         conversion:domain_template "[_#1_]";
         #conversion:domain_name     "Thing";
      ];
      #conversion:enhance [
      #   conversion:class_name "Thing";
      #   conversion:subclass_of <http://purl.org/...>;
      #];
      conversion:enhance [
         ov:csvCol          1;
         ov:csvHeader       "Names";
         a conversion:Only_if_column;
         #conversion:label   "Names";
         conversion:equivalent_property foaf:name;
         conversion:comment "";
         conversion:delimits_object ",\\s*";
         conversion:range   rdfs:Literal;
      ];
      conversion:enhance [
         ov:csvCol          2;
         ov:csvHeader       "linkedmdb";
         #conversion:label   "linkedmdb";
         conversion:equivalent_property owl:sameAs;
         conversion:comment "";
         conversion:range   rdfs:Resource;
      ];
      conversion:enhance [
         ov:csvCol          3;
         ov:csvHeader       "imdb";
         #conversion:label   "imdb";
         conversion:equivalent_property foaf:page;
         conversion:comment "";
         conversion:range   rdfs:Resource;
      ];
      conversion:enhance [
         ov:csvCol          4;
         ov:csvHeader       "wikia";
         #conversion:label   "wikia";
         conversion:equivalent_property foaf:page;
         conversion:comment "";
         conversion:range   rdfs:Resource;
      ];
      conversion:enhance [
         ov:csvCol          5;
         ov:csvHeader       "dbpedia";
         #conversion:label   "dbpedia";
         conversion:equivalent_property owl:sameAs;
         conversion:comment "";
         conversion:range   rdfs:Resource;
      ];
      #conversion:enhance [            
      #   ov:csvRow 3,4,5;             
      #   a conversion:ExampleResource;
      #];                              
   ];
